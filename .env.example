# Google OAuth (Get from Google Console)
GOOGLE_CLIENT_ID_LOCAL=your_google_client_id_for_development
GOOGLE_CLIENT_SECRET_LOCAL=your_google_client_secret_for_development
GOOGLE_CLIENT_ID_PROD=your_google_client_id_for_production
GOOGLE_CLIENT_SECRET_PROD=your_google_client_secret_for_production

# GitHub OAuth (Get from GitHub Developer Settings)
GITHUB_CLIENT_ID_LOCAL=your_github_client_id_for_development
GITHUB_CLIENT_SECRET_LOCAL=your_github_client_secret_for_development
GITHUB_CLIENT_ID_PROD=your_github_client_id_for_production
GITHUB_CLIENT_SECRET_PROD=your_github_client_secret_for_production

# Better Auth Secret (Generate a random 32+ character string)
BETTER_AUTH_SECRET=your_better_auth_secret_key_min_32_characters

# Database URL (PostgreSQL connection string)
DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# App URL (Your domain)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Environment selector (development/production)
AUTH_ENV=development
